#define PRESSURE_PIN 4

void setup() {
  Serial.begin(9600);
}

void loop() {
  int raw = analogRead(PRESSURE_PIN);
  float voltage = raw * (3.3 / 4095.0);

  Serial.print("ADC: ");
  Serial.print(raw);
  Serial.print("  Voltage: ");
  Serial.print(voltage, 2);
  Serial.println(" V");

  delay(500);
}
